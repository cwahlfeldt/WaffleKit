{
  "name": "WaffleKit",
  "version": "0.0.1",
  "description": "Starter kit for React, Redux, Webpack, Babel, Bootstrap and Sass.",
  "main": "index.js",
  "author": "Chris Wahlfeldt",
  "license": "MIT",
  "scripts": {
    "start": "cd ./webpack && webpack-dev-server --env=dev",
    "build:dev": "rm -rf ./build && cd webpack && webpack --env=dev --progress --profile --colors",
    "build:prod": "rm -rf ./build && cd webpack && webpack --env=prod --progress --profile --colors",
    "cleanup": "rm -rf ./build"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  }
}
